{
    "name": "Hide Amazon Sponsored Product",
    "version": "1.0.0",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": [ "https://www.amazon.co.jp/*" ],
            "js": [ "content.js" ]
        }
    ],
    "background": {
        "service_worker": "service_worker.js",
        "type": "module"
    }
}
